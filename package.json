{
  "name": "starter-react",
  "version": "0.0.1",
  "description": "A small scaffold to get you started using React with Webpack",
  "main": "server.js",
  "scripts": {
    "old-build": "webpack",
    "old-start": "webpack --mode development --watch --info-verbosity verbose",
    "old-serve": "node server.js",
    "start": "webpack-dev-server --open --config new-webpack/new-webpack.config.dev.js --info-verbosity verbose",
    "build": "webpack --config new-webpack/new-webpack.config.prod.js --colors",
    "serve": "node server.js",
    "new-build": "webpack --config webpack/webpack.config.prod.js  --colors",
    "new-start": "webpack-dev-server --open --config webpack/webpack.config.dev.js"
  },
  "devDependencies": {
    "babel": "latest",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "latest",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash-es": "^4.17.14",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.24.0",
    "style-loader": "latest",
    "svg-inline-loader": "^0.8.0",
    "webpack": "latest",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "angular": "^1.6.6",
    "angular-animate": "^1.6.6",
    "angular-aria": "^1.6.6",
    "angular-material": "^1.1.10",
    "angular-messages": "^1.6.6",
    "angular-route": "^1.6.6",
    "angular-sanitize": "^1.6.6",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.13",
    "node-sass": "^4.9.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/starter-react"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
